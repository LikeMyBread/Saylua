{
  "name": "saylua",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "js-lint": "eslint frontend/source/**/*.js*",
    "py-lint": "flake8 saylua/ --statistics --count",
    "lint": "npm run js-lint && npm run py-lint",
    "js-test": "mocha --compilers js:babel-register frontend/source/**/tests/*.js*",
    "py-test": "python -m pytest",
    "test": "npm run lint && npm run py-test && npm run js-test",
    "js-deps": "npm install && npm install -g eslint eslint-plugin-inferno mocha webpack",
    "py-deps": "pip install -r requirements.txt -t lib/main",
    "deps": "npm run js-deps && npm run py-deps",
    "build-css": "node-sass-chokidar frontend/source/ -o frontend/static/",
    "watch-css": "npm run build-css && node-sass-chokidar frontend/source/ -o frontend/static/ --watch --recursive",
    "build-js": "webpack",
    "watch-js": "webpack --watch",
    "start": "dev_appserver.py .",
    "deploy": "appcfg.py -A saylua-staging update .",
    "deploy-prod": "appcfg.py -A saylua-1280 update ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/saylua/saylua.git"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-inferno": "^3.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "es6-promise": "^4.1.1",
    "eslint": "^4.2.0",
    "eslint-plugin-inferno": "^7.0.1",
    "inferno": "^3.6.4",
    "inferno-component": "^3.6.4",
    "inferno-test-utils": "^3.6.4",
    "mocha": "^3.4.2",
    "mocha-webpack": "^0.7.0",
    "path": "^0.12.7",
    "pseudomap": "^1.0.2",
    "redux": "^3.7.2",
    "webpack": "^3.3.0",
    "whatwg-fetch": "^2.0.3"
  }
}
